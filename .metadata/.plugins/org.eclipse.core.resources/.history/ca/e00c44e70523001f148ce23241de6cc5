package com.rahul;

public class DrawingApp {
	private Circle c;

	public DrawingApp() {
		super();
		// TODO Auto-generated constructor stub
	}

	public DrawingApp(Circle c) {
		super();
		this.c = c;
	}

	public Circle getC() {
		return c;
	}

	public void setC(Circle c) {
		this.c = c;
	}

	@Override
	public String toString() {
		return "DrawingApp [c=" + c + "]";
	}

	public void draw() {
		System.out.println(c);
	}

}
